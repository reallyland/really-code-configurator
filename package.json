{
  "name": "really-code-configurator",
  "version": "0.0.0",
  "private": true,
  "description": "Configure and generate code snippet for custom element",
  "keywords": [
    "really-code-configurator",
    "lit-element",
    "lit-html",
    "polymer",
    "typescript",
    "web-components"
  ],
  "homepage": "https://github.com/reallyland/really-code-configurator",
  "repository": {
    "type": "git",
    "url": "git@github.com:reallyland/really-code-configurator.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng (motss)",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/my-element.js",
  "module": "./dist/my-element.js",
  "typings": "./dist/my-element.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.test.json",
    "clean": "rm -rf ./*.*js ./*.d.ts dist/",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish",
    "prepublishOnly": "rm -rf dist/ && rollup -c ./rollup.config.js",
    "rollup": "rollup -c ./rollup.config.js",
    "test": "npm run lint && npm run build && wct",
    "ts": "tsc -p ./tsconfig.json",
    "wct": "wct --skip-selenium-install --config-file ./wct.conf.json",
    "watch": "npm run ts -- --watch"
  },
  "dependencies": {
    "@material/mwc-button": "^0.5.0",
    "lit-element": "^2.1.0",
    "lit-html": "^1.0.0",
    "prismjs": "^1.16.0"
  },
  "devDependencies": {
    "@messageflow/tslint-config": "^1.3.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/prismjs": "^1.16.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.2",
    "wct-mocha": "^1.0.0"
  },
  "engines": {
    "node": ">= 8.16.0",
    "npm": ">= 6.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
